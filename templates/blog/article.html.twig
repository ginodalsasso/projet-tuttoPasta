{% extends 'base.html.twig' %}

{% block title %}{{article.articleTitle}}{% endblock %}

{% block body %}

    <h1>Article</h1>
    {{article.articleTitle}} <br>
    {{article.articleContent}} <br>
    {% for category in article.categories %}
        <p>{{category.categoryName}}</p>
    {% endfor %}
        <p>{{article.date}}</p>

{% endblock %}
